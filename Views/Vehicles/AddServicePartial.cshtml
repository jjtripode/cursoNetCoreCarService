@model AddServiceViewModel


<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#exampleModalCenter">
    Agregar
</button>
<form asp-action="AddVehicle" method="POST">
    <div class="modal fade" id="exampleModalCenter" tabindex="-1" role="dialog" aria-labelledby="exampleModalCenterTitle" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle">Modal title</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <label for="Date" asp-for="Date" class="col-form-label">Patente</label>
                        <input asp-for="Date">
                    </div>
           
                    <div class="form-group">
                        <label for="Price" asp-for="Price" class="col-form-label">Patente</label>
                        <input asp-for="Price">
                    </div>

           

                                  <div class="form-group">
                        <label for="Status" asp-for="Status" class="col-form-label">Fecha Fabricacion</label>
                        <input asp-for="Status">
                    </div>



                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary">Guardars</button>
                </div>
            </div>
        </div>
    </div>
</form>





